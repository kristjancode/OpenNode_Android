<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:orientation="horizontal"
	android:layout_height="65dp">
	<RelativeLayout android:id="@+id/relativeLayout1"
		android:layout_height="match_parent" android:layout_width="match_parent">
		<ImageView android:layout_width="40dp" android:id="@+id/managerView"
			android:layout_height="40dp" android:src="@drawable/empty48"
			android:layout_centerVertical="true" android:layout_alignParentRight="true"></ImageView>
		<TextView android:textSize="20dp" android:id="@+id/computeRow"
			android:text="Empty" android:layout_width="wrap_content"
			android:keepScreenOn="true" android:layout_height="wrap_content"
			android:layout_centerVertical="true" android:layout_toRightOf="@+id/icon"
			android:layout_toLeftOf="@+id/managerView" android:layout_marginLeft="5dp"
			android:maxLength="20"></TextView>
		<ImageView android:layout_width="40dp" android:id="@+id/icon"
			android:layout_height="40dp" android:src="@drawable/start48"
			android:layout_centerVertical="true" android:layout_alignParentLeft="true"
			android:layout_marginLeft="2dp"></ImageView>

	</RelativeLayout>

</LinearLayout>