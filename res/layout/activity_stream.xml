<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget103"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:layout_width="match_parent"
		android:id="@+id/linearLayout1" android:layout_height="wrap_content">
		<TextView android:id="@+id/activity_label"
			android:background="@color/actionbar_background_end"
			android:typeface="serif" android:padding="12px" android:text="Activity Stream"
			android:textSize="24sp" android:layout_width="272dp" android:gravity="center"
			android:layout_height="wrap_content"></TextView>
		<ImageView android:id="@+id/btn_computeSearch"
			android:layout_width="match_parent" android:background="@color/actionbar_background_end"
			android:layout_height="match_parent" android:src="@drawable/search48"></ImageView>
	</LinearLayout>
	<ListView android:id="@+id/list_activity"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
	</ListView>
</LinearLayout>
