<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="5dip" />
			<gradient android:startColor="#ff9d9e9d"
				android:centerColor="#ff5a5d5a" android:centerY="0.75"
				android:endColor="#ff747674" android:angle="270" />
		</shape>
	</item>

	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="5dip" />
				<gradient android:startColor="#ff39b54a"
					android:centerColor="#ff77b539" android:centerY="0.4"
					android:endColor="#fff22222" android:angle="0" />
			</shape>
		</clip>
	</item>

</layer-list>